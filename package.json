	"engines": {
		"node": ">=10"
	},
	"keywords": [
		"max msp",
		"maxmsp"
	],
	"peerDependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^25.0.7",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@rollup/plugin-typescript": "^11.1.5",
		"@svgr/rollup": "^8.1.0",
		"@svgr/webpack": "^8.1.0",
		"@types/node": "^20.8.7",
		"@types/react": "^18.2.31",
		"@types/react-dom": "^18.2.14",
		"@vitejs/plugin-react-swc": "^3.4.0",
		"gh-pages": "^6.0.0",
		"postcss": "^8.4.31",
		"prettier": "^3.0.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"rollup-plugin-postcss": "^4.0.2",
		"sass": "^1.69.4",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^4.5.0",
		"vite-plugin-gh-pages": "^0.4.1"
	},
	"files": [
		"dist"
	],
	"scripts": {
