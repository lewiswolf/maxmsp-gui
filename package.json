{
	"name": "maxmsp-gui",
	"version": "2.3.0",
	"description": "React component library for stylised Max MSP GUI.",
	"author": "Lewis Wolstanholme",
	"license": "n/a",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/lewiswolf/maxmsp-gui.git"
	},
	"main": "dist/index.js",
	"source": "src/index.ts",
	"types": "dist/index.d.ts",
	"type": "module",
	"private": true,
	"engines": {
		"node": ">=10"
	},
	"keywords": [
		"max msp",
		"maxmsp"
	],
	"peerDependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@types/node": "^20.8.10",
		"@types/react": "^18.2.33",
		"@types/react-dom": "^18.2.14",
		"@typescript-eslint/eslint-plugin": "^6.9.1",
		"@typescript-eslint/parser": "^6.9.1",
		"@vitejs/plugin-react-swc": "^3.4.1",
		"eslint": "^8.52.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.4",
		"gh-pages": "^6.0.0",
		"prettier": "^3.0.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"sass": "^1.69.5",
		"typescript": "^5.2.2",
		"vite": "^4.5.0",
		"vite-plugin-css-injected-by-js": "^3.3.0",
		"vite-plugin-dts": "^3.6.3",
		"vite-plugin-svgr": "^4.1.0"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "vite build --watch",
		"build": "tsc && vite build",
		"example": "cd example && npm run dev",
		"lint": "npx prettier --write --config ./.prettierrc . ",
		"test": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 && tsc",
		"predeploy": "npm run build && cd example && npm run build",
		"deploy": "gh-pages -d example/dist"
	}
}
