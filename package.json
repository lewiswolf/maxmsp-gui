		"maxmsp"
	],
	"peerDependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^25.0.2",
		"@rollup/plugin-node-resolve": "^15.1.0",
		"@rollup/plugin-typescript": "^11.1.1",
		"@svgr/rollup": "^8.0.1",
		"@svgr/webpack": "^8.0.1",
		"@types/node": "^20.3.1",
		"@types/react": "^18.2.13",
		"@types/react-dom": "^18.2.6",
		"@vitejs/plugin-react-swc": "^3.3.2",
		"gh-pages": "^5.0.0",
		"postcss": "^8.4.24",
		"prettier": "^2.8.8",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"rollup-plugin-postcss": "^4.0.2",
		"sass": "^1.63.4",
		"tslib": "^2.5.3",
		"typescript": "^5.1.3",
		"vite": "^4.3.9",
		"vite-plugin-gh-pages": "^0.4.1"
	},
	"files": [
		"dist"
	],
	"scripts": {
