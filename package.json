{
	"name": "maxmsp-gui",
	"version": "2.2.3",
	"description": "React component library for stylised Max MSP GUI.",
	"author": "Lewis Wolstanholme",
	"license": "n/a",
	"repository": "lewiswolf/maxmsp-gui",
	"main": "dist/index.js",
